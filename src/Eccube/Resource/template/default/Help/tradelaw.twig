                    </dl>

                    <dl id="trade_term04">
                        <dt>{{'tradelaw.label.payment_due'|trans}}</dt>
                        <dd></dd>
                    </dl>

                    <dl id="trade_term05">
                        <dt>{{'tradelaw.label.delivery_timing'|trans}}</dt>
                        <dd></dd>
                    </dl>

                    <dl id="trade_term06">
                        <dt>{{'tradelaw.label.return_exchange_policy'|trans}}</dt>
                        <dd></dd>
                    </dl>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
