{% extends '@WebProfiler/Profiler/layout.html.twig' %}


{% block toolbar %}
<style>
.sf-toolbarreset {
    z-index: 100026; // $zindex-veryhigh + 26
    background-color: #4C505C;
}
.sf-minitoolbar {
    background-color: transparent;
    background-image: url('data:image/svg+xml;base64,');
    background-size: 32px 32px;
    background-repeat: no-repeat;
    z-index: 100026; // $zindex-veryhigh + 26
}
.sf-minitoolbar svg {
    opacity: 0;
}
.sf-toolbar-icon img {
    max-height: 32px;
}
</style>

    {% set icon %}
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 124.45 105.65"><defs><style>.cls-1{fill:#353a4e;}.cls-2{fill:#fc0;}.cls-3{fill:#f2b50a;}</style></defs><title>eccube_logo_basic</title><path class="cls-1" d="M106.81,15s6.63,49.2-37.4,62.22c-4.09,0-56.94,6.54-58.6-46.42V81.11L69.4,95,110.81,77V16.58Z"/><path class="cls-2" d="M110.81,63.42V77L69.4,95,10.81,81.11V65C4,69.18,0,74.22,0,79.65c0,14.36,27.86,26,62.22,26s62.22-11.64,62.22-26C124.45,73.51,119.33,67.86,110.81,63.42Z"/><path class="cls-2" d="M69.4,0,10.81,25C9,84.13,61.55,78.42,69.4,77.21Z"/><path class="cls-3" d="M106.81,15,69.4,0V77.21C115.22,65.84,106.81,15,106.81,15Z"/></svg>

        <span class="sf-toolbar-label">
            <strong>EC-CUBE {{ collector.version }}</strong>
        </span>

    {% endset %}

    {% set text %}

        <div class="sf-toolbar-info-piece">
            <span>
                <strong>EC-CUBE</strong> {{ collector.payoff }}
            </span>
        </div>

        <div class="sf-toolbar-info-piece">
            <span>
                Version: {{ collector.version }} - {{ collector.dashboardlink|raw }} -
                Visit <a href="https://www.ec-cube.net" target="_blank">ec-cube.net</a>
            </span>
        </div>

        {% if collector.branding is not empty %}
        <div class="sf-toolbar-info-piece">

            <span>
                {{ collector.branding|raw }}
            </span>

        </div>
       {% endif %}

        {% if collector.editlink %}
        <div class="sf-toolbar-info-piece">
            <hr style="margin: 8px 0 12px; padding: 0; border: 0; border-bottom: 1px solid #777;">
            <span> Edit: <a href="{{ collector.editlink }}">{{ collector.edittitle|default("(no title)") }}</a> </span>
        </div>
        {% endif %}


    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

